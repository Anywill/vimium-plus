{
  "author": "gdh1995@qq.com",
  "background": {
    "persistent": true,
    "scripts": [
      "background/utils.js",
      "background/settings.js",
      "background/main.js",
      "background/commands.js",
      "background/exclusions.js",
      "background/tools.js",
      "background/completion.js",
      "background/others.js"
    ]
  },
  "browser_action": {
    "default_icon": {
      "19": "icons/enabled_19.png",
      "38": "icons/enabled_38.png"
    },
    "default_popup": "pages/popup.html",
    "default_title": "Vimium++"
  },
  "chrome_url_overrides": {
    "newtab": "pages/newtab.html"
  },
  "commands": {
    "createTab": {
      "description": "Create new tab(s)",
      "suggested_key": {
        "default": "Alt+T"
      }
    },
    "previousTab": {
      "description": "Go one tab left",
      "suggested_key": {
        "default": "Alt+C"
      }
    },
    "nextTab": {
      "description": "Go one tab right",
      "suggested_key": {
        "default": "Alt+V"
      }
    },
    "reloadTab": {
      "description": "Reload tab(s)",
      "suggested_key": {
        "default": "Alt+R"
      }
    }
  },
  "content_scripts": [ {
    "all_frames": true,
    "js": [
      "lib/utils.js",
      "lib/keyboard_utils.js",
      "lib/dom_utils.js",
      "content/dom_ui.js",
      "content/link_hints.js",
      "content/vomnibar.js",
      "content/scroller.js",
      "content/marks.js",
      "content/frontend.js",
      "content/mode_find.js",
      "lib/extend_click.js"
    ],
    "matches": [ "<all_urls>" ],
    "run_at": "document_start"
  }, {
    "all_frames": true,
    "css": [ "front/file_urls.css" ],
    "matches": [ "file:///*" ],
    "run_at": "document_idle"
  } ],
  "content_security_policy": "script-src 'self'; object-src 'self';",
  "description": "optimized Vimium supporting global commands and other extensions",
  "homepage_url": "https://github.com/gdh1995/vimium-plus",
  "icons": {
    "128": "icons/icon128.png",
    "16": "icons/icon16.png",
    "48": "icons/icon48.png"
  },
  "key": "",
  "manifest_version": 2,
  "name": "Vimium++",
  "omnibox": {
    "keyword": "v"
  },
  "options_page": "pages/options.html",
  "permissions": [
    "bookmarks",
    "clipboardRead",
    "contentSettings",
    "history",
    "notifications",
    "sessions",
    "storage",
    "tabs",
    "webNavigation",
    "<all_urls>"
  ],
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "1.54",
  "web_accessible_resources": [
    "content/*",
    "lib/*"
  ]
}
